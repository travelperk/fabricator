{
  "name": "@travelperksl/fabricator",
  "version": "1.0.0",
  "description": "Fabricate objects for your tests",
  "keywords": [
    "fabricator",
    "fabrication",
    "fabricate",
    "testing"
  ],
  "main": "dist/fabricator.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel src -d dist --extensions .ts",
    "clean": "rm -rf dist",
    "extract-types": "tsc --emitDeclarationOnly -p tsconfig.json",
    "prepare": "husky install",
    "prettier": "prettier",
    "semantic-release": "semantic-release",
    "test": "jest --coverage",
    "test:ci": "jest --coverage --ci",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/travelperk/fabricator.git"
  },
  "bugs": {
    "url": "https://github.com/travelperk/fabricator/issues"
  },
  "homepage": "https://github.com/travelperk/fabricator#readme",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "29.2.3",
    "babel-jest": "^29.3.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "semantic-release": "^19.0.5",
    "typescript": "4.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
